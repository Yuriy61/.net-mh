                </div>
            </div>
            <div class="col-md-7">
                <div class="title_right">
                    <ul>
                        <li><a href="javascript:void(0)" onclick="clickCategory(1,'',this)">工作台</a></li>
                        <li><a href="javascript:void(0)" onclick="clickCategory(7,'', this)">铝型材框架应用</a></li>
                        <li><a href="javascript:void(0)" onclick="clickCategory(0,'30', this)">铝型材防护围栏</a></li>
                        <li><a href="javascript:void(0)" onclick="clickCategory(5, '',this)">自动化设备</a></li>
                        <li><a href="javascript:void(0)" onclick="clickCategory(0,'47,9', this)">流水线</a></li>
                    </ul>
                    <a href="/Product/List?id=1" id="proMore">MORE</a>
                </div>
                <div class="pro_list">
                    <div id="pc">
                        <div class="row">
                            @{
                                int i1 = 0;
                                foreach (var item in ViewBag.ProList1 as List<AMW.Model.Entity.MldProduct>)
                                {
                                    i1++;
                                    <div class="col-md-4">
                                        <div class="diva">
                                            <div class="imgbg">
                                                <img src="@item.Img" />
                                                <a href="javascript:void(0)" onclick="window.open('http://wpa.qq.com/msgrd?V=1&uin=2880260703'); return false;" class="zx">立即咨询</a>
                                                <a href="@("/Product/Index/" + item.ID)" class="lj">了解详情</a>
                                            </div>
                                            <p>@item.Name</p>
                                        </div>
                                    </div>
                                    if (i1 % 3 == 0)
                                    {
                                        @Html.Raw("</div><div class=\"row\">");

                                    }
                                }
                            }
                        </div>
                    </div>
                </div>
            </div>
