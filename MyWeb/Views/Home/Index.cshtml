                    .pro_info .title_list > a:hover{
                        background-color:#deeae6;
                        border:1px dashed #048b95;
                        color:#018997;
                    }
                    .pro_info .title_right>ul li{
                        width:auto !important;
                        padding:0 15px;
                    }
                    .pro_info .title_list > a{
                        margin-top: 15px;
                        height: 57px;
                        line-height:57px;
                    }
                </style>
                <script type="text/javascript">
                    function clickCategory(id,cid,obj) {
                        $(".pro_info .title_right>ul li a").removeClass("cur");
                        $(obj).addClass("cur");

                        $.post("/Home/GetHomeProduct2", { id:id,cid: cid }).done(function (d) {
                            $("#pc").html("");
                            if (!!d && !!d.ok && !!d.data) {
                                if (!!d.data.proList && !!d.data.proList.length > 0) {
                                    var idx = 0;
                                    var html = "<div class=\"row\">";
                                    d.data.proList.forEach(function (pro) {
                                        idx++;
                                        html += "<div class=\"col-md-4\"><div class=\"diva\">" +
                                                        "<div class=\"imgbg\">" +
                                                            "<img src=\"" + pro.Img + "?r=22111\">" +
                                                            "<a href=\"javascript:void(0)\" onclick=\"window.open('http://wpa.qq.com/msgrd?V=1&amp;uin=2880260703'); return false;\" class=\"zx\">立即咨询</a>" +
                                                            "<a href=\"/Product/Index/" + pro.ID + "\" class=\"lj\">了解详情</a>" +
                                                        "</div><p>" + pro.Name + "</p></div></div>";
                                        if (idx % 3 == 0) {
                                            html += "</div><div class=\"row\">";
                                        }
                                    });
                                    html += "</div>";
                                    $("#pc").html(html);
                                }
                            }
                        });
                    }
                </script>
                <div class="title_list">
                    <a href="/Product/List?id=1">工作台</a>
                    <a href="/Product/List?id=2">工业铝型材</a>
                    <a href="/Product/List?id=3">围栏及防护罩</a>
                    <a href="/Product/List?id=4">工业梯/踏台/工业平台</a>
